w": "Confederate States of America", "machine_tag": 0, "id": "31814686-5840583976-1298495", "_content": "confederatestatesofamerica", "author": "25695066@N00"}, {"raw": "C.S.A.", "machine_tag": false, "id": "31814686-5840583976-127008", "_content": "csa", "author": "25695066@N00"}, {"raw": "The Confederacy", "machine_tag": 0, "id": "31814686-5840583976-13788191", "_content": "theconfederacy", "author": "25695066@N00"}, {"raw": "Confederacy", "machine_tag": false, "id": "31814686-5840583976-65230", "_content": "confederacy", "author": "25695066@N00"}, {"raw": "Confederate", "machine_tag": false, "id": "31814686-5840583976-10912", "_content": "confederate", "author": "25695066@N00"}, {"raw": "The South", "machine_tag": 0, "id": "31814686-5840583976-23540", "_content": "thesouth", "author": "25695066@N00"}, {"raw": "Rebel", "machine_tag": 0, "id": "31814686-5840583976-1973", "_content": "rebel", "author": "25695066@N00"}, {"raw": "Rebels", "machine_tag": false, "id": "31814686-5840583976-117553", "_content": "rebels", "author": "25695066@N00"}, {"raw": "Soldier", "machine_tag": 0, "id": "31814686-5840583976-1825", "_content": "soldier", "author": "25695066@N00"}, {"raw": "Sabre", "machine_tag": 0, "id": "31814686-5840583976-13297", "_content": "sabre", "author": "25695066@N00"}]}, "comments": {"_content": "0"}, "geoperms": {"isfriend": 0, "isfamily": 0, "ispublic": 1, "iscontact": 0}, "secret": "0b82c5e5ef", "location": {"locality": {"woeid": "2477870", "place_id": "MUMTgGFTVr0pvc4E", "_content": "Quincy"}, "country": {"woeid": "23424977", "place_id": "nz.gsghTUb4c2WAecA", "_content": "United States"}, "region": {"woeid": "2347568", "place_id": "IPbJb2lTUb4Y7tKw", "_content": "Florida"}, "place_id": "MUMTgGFTVr0pvc4E", "longitude": -84.607597, "county": {"woeid": "12587822", "place_id": "Kt6R6GVQUL_ouTbSjQ", "_content": "Gadsden"}, "context": "0", "woeid": "2477870", "latitude": 30.549829, "accuracy": "11"}, "usage": {"canblog": 0, "canshare": 1, "candownload": 1, "canprint": 0}, "description": {"_content": "<strong>Local call number:</strong> Rc11562\n\n<strong>Title:</strong> Portrait of Hugh Black: Gadsden County\n\n<strong>Date:</strong> 186-\n\n<strong>Physical descrip:</strong> 1 photoprint; b&amp;w; 10 x 8 in.\n\n<strong>Series Title:</strong> Reference collection\n\n<strong>General note:</strong> Born in Gadsden County on June 28, 1835, as the son of James Black. He fought with Captain Parkhill's volunteers in the Everglades in the third Seminole War (1855-58). Hugh married Mary Ann Harvey of Leon County in 1860. They moved to Liberty County and he became the tax assessor there until the start of the Civil War. He volunteered and was captain of the Liberty Rangers, an infantry outfit. He died on May 12, 1915.\n\n<strong>Repository:</strong> State Library and Archives of Florida, 500 S. Bronough St., Tallahassee, FL 32399-0250 USA. Contact: 850.245.6700. Archives@dos.state.fl.us \n\n<strong>Persistent URL:</strong> <a href=\"http://www.floridamemory.com/items/show/34057\" rel=\"nofollow\">www.floridamemory.com/items/show/34057</a> \n"}, "isfavorite": 0, "views": "1796", "farm": 4, "visibility": {"isfriend": 0, "isfamily": 0, "ispublic": 1}, "rotation": 0, "dates": {"taken": "1861-00-01 00:00:00", "lastupdate": "1378951105", "takengranularity": "8", "posted": "1308257062"}, "originalsecret": "53187b7cd1", "license": "7", "notes": {"note": []}, "server": "3402", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/floridamemory/5840583976/"}]}, "editability": {"canaddmeta": 0, "cancomment": 0}}, "stat": "ok", "flarchive:created": 1387665310}