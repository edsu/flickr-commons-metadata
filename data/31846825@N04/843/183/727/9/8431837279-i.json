uthor": "31846825@N04"}, {"raw": "abolitionists", "machine_tag": 0, "id": "31814686-8431837279-683705", "_content": "abolitionists", "author": "31846825@N04"}, {"raw": "abolitionism", "machine_tag": false, "id": "31814686-8431837279-804356", "_content": "abolitionism", "author": "31846825@N04"}, {"raw": "John Quincy Adams", "machine_tag": 0, "id": "31814686-8431837279-334080", "_content": "johnquincyadams", "author": "31846825@N04"}, {"raw": "U.S. Supreme Court", "machine_tag": 0, "id": "31814686-8431837279-58647", "_content": "ussupremecourt", "author": "31846825@N04"}, {"raw": "human rights", "machine_tag": 0, "id": "31814686-8431837279-35184", "_content": "humanrights", "author": "31846825@N04"}, {"raw": "Key West", "machine_tag": 0, "id": "31814686-8431837279-25416", "_content": "keywest", "author": "31846825@N04"}, {"raw": "Monroe County", "machine_tag": 0, "id": "31814686-8431837279-422315", "_content": "monroecounty", "author": "31846825@N04"}]}, "comments": {"_content": "1"}, "geoperms": {"isfriend": 0, "isfamily": 0, "ispublic": 1, "iscontact": 0}, "secret": "22b53f67fe", "location": {"locality": {"woeid": "2432044", "place_id": "h.4G3KFTVrkDcyB.", "_content": "Key West"}, "country": {"woeid": "23424977", "place_id": "nz.gsghTUb4c2WAecA", "_content": "United States"}, "region": {"woeid": "2347568", "place_id": "IPbJb2lTUb4Y7tKw", "_content": "Florida"}, "place_id": "h.4G3KFTVrkDcyB.", "longitude": -81.806817, "county": {"woeid": "12587846", "place_id": "FnGAWIBQUL.LwGTNQA", "_content": "Monroe"}, "context": "0", "woeid": "2432044", "latitude": 24.561351, "accuracy": "16"}, "usage": {"canblog": 0, "canshare": 1, "candownload": 1, "canprint": 0}, "description": {"_content": "<strong>Local call number:</strong> DM5661\n\n<strong>Title:</strong> Replica of the slave ship <em>Amistad</em> docked at Key West\n\n<strong>General note:</strong> The <em>Freedom Schooner Amistad</em> is a reconstruction of the historic coastal cargo schooner <em>La Amistad</em> that in 1839 transported 53 kidnapped West Africans. The captives revolted and seized the ship, but were eventually recaptured off the coast of New England where abolitionists took up their cause for freedom. Former President John Quincy Adams successfully argued before the U.S. Supreme Court on behalf of the captives in the first human rights case to be argued in the American court system on behalf of Africans. In 1841, the 35 surviving Africans were returned to Africa.\n\n<strong>Date:</strong> January 11, 2003\n\n<strong>Physical descrip:</strong> 1 photonegative - col. - 35 mm.\n\n<strong>Series Title:</strong> <a href=\"http://floridamemory.com/photographiccollection/collections/?id=28\" rel=\"nofollow\">Dale M. McDonald Collection</a>\n\n<strong>Repository:</strong>  <a href=\"http://www.floridamemory.com/\" rel=\"nofollow\">State  Library and Archives of Florida</a>, 500 S. Bronough St., Tallahassee, FL  32399-0250 USA. Contact: 850.245.6700. Archives@dos.myflorida.com\n\n<strong>Persistent URL:</strong> <a href=\"http://floridamemory.com/items/show/101216\" rel=\"nofollow\">floridamemory.com/items/show/101216</a>\n"}, "isfavorite": 0, "views": "2481", "farm": 9, "visibility": {"isfriend": 0, "isfamily": 0, "ispublic": 1}, "rotation": 0, "dates": {"taken": "2003-01-11 05:59:31", "lastupdate": "1362809827", "takengranularity": "0", "posted": "1359640771"}, "originalsecret": "e45ce0e3ce", "license": "7", "notes": {"note": []}, "server": "8230", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/floridamemory/8431837279/"}]}, "editability": {"canaddmeta": 0, "cancomment": 0}}, "stat": "ok", "flarchive:created": 1387664091}