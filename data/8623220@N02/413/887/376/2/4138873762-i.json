}, "originalsecret": "037dab32c7", "license": "7", "notes": {"note": [{"_content": "New York City Mayor William J. Gaynor, being publicly notified of his nomination for re-election by a delegation of indepentent voters. He died one week after this photo was taken.", "author": "70251312@N00", "h": "192", "authorname": "Wystan", "w": "64", "y": "72", "x": "117", "id": "72157622813006313"}, {"_content": "Symbolic spade, for burying Tammany.", "author": "70251312@N00", "h": "102", "authorname": "Wystan", "w": "57", "y": "174", "x": "180", "id": "72157622937526792"}, {"_content": "Photo is dated 9/3/13.", "author": "70251312@N00", "h": "50", "authorname": "Wystan", "w": "50", "y": "6", "x": "254", "id": "72157622937530932"}, {"_content": "R. Ross Appleton, manager of Gaynor's re-election campaign.", "author": "70251312@N00", "h": "56", "authorname": "Wystan", "w": "43", "y": "120", "x": "178", "id": "72157622813209887"}]}, "server": "2561", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/library_of_congress/4138873762/"}]}, "editability": {"canaddmeta": 0, "cancomment": 0}}, "stat": "ok"}