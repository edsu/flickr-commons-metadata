: 2, "visibility": {"isfriend": 0, "isfamily": 0, "ispublic": 1}, "rotation": 0, "dates": {"taken": "1918-01-01 00:00:00", "lastupdate": "1277497203", "takengranularity": "8", "posted": "1276535614"}, "originalsecret": "4e53486b33", "license": "7", "notes": {"note": []}, "server": "1274", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/nlscotland/4699937617/"}]}, "editability": {"canaddmeta": 0, "cancomment": 0}}, "stat": "ok"}