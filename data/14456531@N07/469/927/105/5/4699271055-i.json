, "visibility": {"isfriend": 0, "isfamily": 0, "ispublic": 1}, "rotation": 0, "dates": {"taken": "1918-01-01 00:00:00", "lastupdate": "1328943255", "takengranularity": "8", "posted": "1276521260"}, "originalsecret": "78b86a31ae", "license": "7", "notes": {"note": []}, "server": "1273", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/nlscotland/4699271055/"}]}, "editability": {"canaddmeta": 0, "cancomment": 0}}, "stat": "ok"}