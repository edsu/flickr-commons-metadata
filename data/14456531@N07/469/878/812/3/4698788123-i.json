es": {"taken": "1918-01-01 00:00:00", "lastupdate": "1282770241", "takengranularity": "8", "posted": "1276505367"}, "originalsecret": "24fa65cbe2", "license": "7", "notes": {"note": []}, "server": "4052", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/nlscotland/4698788123/"}]}, "editability": {"canaddmeta": 0, "cancomment": 0}}, "stat": "ok"}