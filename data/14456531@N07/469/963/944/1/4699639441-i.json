s": {"isfriend": 0, "isfamily": 0, "ispublic": 1, "iscontact": 0}, "secret": "b89b44155a", "location": {"locality": {"woeid": "588990", "place_id": "0vxIIDJUWrKnNyM", "_content": "Donchery"}, "country": {"woeid": "23424819", "place_id": "lbWye9tTUb6GOcp80w", "_content": "France"}, "region": {"woeid": "7153315", "place_id": "2mGGPypWU79n_Zk.", "_content": "Champagne-Ardenne"}, "place_id": "0vxIIDJUWrKnNyM", "longitude": 4.87471, "county": {"woeid": "12597142", "place_id": "10hxPapQUL9gTNCM2A", "_content": "Ardennes"}, "context": "0", "woeid": "588990", "latitude": 49.695041, "accuracy": "9"}, "usage": {"canblog": 0, "canshare": 1, "candownload": 1, "canprint": 0}, "description": {"_content": "This photograph, believed to have been taken by John Warwick Brooke, shows a road junction in a village or town. French and British troops, including members of the Medical Corps, are sitting and standing at the roadside, while carts are being driven past by British soldiers.\n\nThe sign in the background reads, '[D]onchery'  with a directional arrow. Donchery is in Ardennes, so the town in the photograph is likely to be nearby, in the area of the Allied advance in the autumn of 1918.\n\n[Original reads: 'OFFICIAL PHOTOGRAPH TAKEN ON THE BRITISH WESTERN FRONT IN FRANCE. French and British on the roadside.']\n\n<a href=\"http://digital.nls.uk/74547652\" rel=\"nofollow\">digital.nls.uk/74547652</a>"}, "isfavorite": 0, "views": "1630", "farm": 5, "visibility": {"isfriend": 0, "isfamily": 0, "ispublic": 1}, "rotation": 0, "dates": {"taken": "1918-01-01 00:00:00", "lastupdate": "1289416663", "takengranularity": "8", "posted": "1276529426"}, "originalsecret": "8c5009c9b4", "license": "7", "notes": {"note": []}, "server": "4048", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/nlscotland/4699639441/"}]}, "editability": {"canaddmeta": 0, "cancomment": 0}}, "stat": "ok"}