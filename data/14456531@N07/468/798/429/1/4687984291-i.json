ts": {"_content": "1"}, "geoperms": {"isfriend": 0, "isfamily": 0, "ispublic": 1, "iscontact": 0}, "secret": "cd2c0a39f7", "location": {"country": {"woeid": "23424819", "place_id": "lbWye9tTUb6GOcp80w", "_content": "France"}, "place_id": "lbWye9tTUb6GOcp80w", "longitude": 1.718189, "context": "0", "woeid": "23424819", "latitude": 46.71067, "accuracy": "4"}, "usage": {"canblog": 0, "canshare": 1, "candownload": 1, "canprint": 0}, "description": {"_content": "Fighting in Champagne. Troops with an Allied tank preparing to go into action, in France, during World War I. British and the French soldiers with one of the tanks that were at the cutting edge of new military technology during World War I. Both Britain and France produced tanks in large numbers in the last years of the war. First used at the Somme in September 1916, the true effectiveness of tanks only became apparent during a successful Allied attack at Cambrai in November 1917.\n\nThe Champagne region of France at the southern end of the Western Front remained one of the most entrenched areas of conflict throughout the war.\n\n[Original reads: 'Fighting in Champagne. One of the Tanks waiting to move forward.']\n\n<a href=\"http://digital.nls.uk/74549150\" rel=\"nofollow\">digital.nls.uk/74549150</a>"}, "isfavorite": 0, "views": "3771", "farm": 5, "visibility": {"isfriend": 0, "isfamily": 0, "ispublic": 1}, "rotation": 0, "dates": {"taken": "1918-01-01 00:00:00", "lastupdate": "1376494613", "takengranularity": "8", "posted": "1276188548"}, "originalsecret": "8ac9a3528e", "license": "7", "notes": {"note": []}, "server": "4031", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/nlscotland/4687984291/"}]}, "editability": {"canaddmeta": 0, "cancomment": 0}}, "stat": "ok"}