arm": 5, "visibility": {"isfriend": 0, "isfamily": 0, "ispublic": 1}, "rotation": 0, "dates": {"taken": "1918-01-01 00:00:00", "lastupdate": "1364253764", "takengranularity": "8", "posted": "1276186623"}, "originalsecret": "ce6e84c250", "license": "7", "notes": {"note": []}, "server": "4022", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/nlscotland/4688541272/"}]}, "editability": {"canaddmeta": 0, "cancomment": 0}}, "stat": "ok"}